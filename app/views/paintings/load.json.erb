<% if @paintings.present? %>
  {
    "data": [
      <% @paintings.each_with_index do |painting, i| %>
        [
          "<%= painting.name %>",
          "<%= painting.price %>",
          "<%= painting.featured %>",
          "<%= escape_javascript(cl_image_tag(painting.image_name, :width => 50, :height => 75, :crop => :fill)) %>"
        ]
        <% if i+1 != @paintings.length %>, <% end %>
      <% end %>
    ]
  }  
<% end %>