<div class="slick-carousel">
  <% for painting in @featured %>
    <%= featured(painting) %>
  <% end %>
</div>
<script>
  $('.slick-carousel').slick({
    autoplay: true,
    //arrows: false,
    pauseOnHover: false,
    autoplaySpeed: 5000,
    fade: true,
    cssEase: 'linear',
    lazyLoad: 'progressive'
  });
</script>